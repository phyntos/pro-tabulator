{
    "name": "pro-tabulator",
    "version": "0.0.14",
    "description": "Pro Tabulator",
    "main": "./dist/pro-tabulator.js",
    "types": "./dist/pro-tabulator.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "antd-less": "lessc --js --modify-var=\"ant-prefix=tabulator\" node_modules/antd/dist/antd.variable.less src/styles/antd.css",
        "table-css-copy": "cp node_modules/@ant-design/pro-table/dist/table.css src/styles",
        "table-css-replace": "replace-in-file --configFile=replace.config.js",
        "styles": "npm run antd-less && npm run table-css-copy && npm run table-css-replace",
        "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
        "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
        "test": "jest --config jestconfig.json",
        "build": "webpack --config webpack.config.ts",
        "prepublishOnly": "npm run styles && npm run prettier && npm run lint && npm run build",
        "start": "webpack serve --open --config webpack.dev.config.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phyntos/pro-tabulator.git"
    },
    "keywords": [],
    "author": "Yeldos Bakytzhan",
    "license": "ISC",
    "devDependencies": {
        "@ant-design/icons": "^4.7.0",
        "@ant-design/pro-table": "^2.80.6",
        "@types/node": "^18.7.16",
        "@types/react": "^17.0.49",
        "@types/react-dom": "^18.0.6",
        "@types/webpack": "^5.28.0",
        "@types/webpack-dev-server": "^4.7.2",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "antd": "^4.23.1",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.1.0",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.8",
        "eslint-plugin-react-hooks": "^4.6.0",
        "html-webpack-plugin": "^5.5.0",
        "less": "^4.1.3",
        "less-loader": "^11.0.0",
        "mini-css-extract-plugin": "^2.6.1",
        "moment": "^2.29.4",
        "null-loader": "^4.0.1",
        "prettier": "^2.7.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "replace-in-file": "^6.3.5",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.3.6",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "peerDependencies": {
        "@ant-design/icons": "=4",
        "@ant-design/pro-table": "=2",
        "antd": "=4",
        "moment": "=2",
        "react": ">=16"
    }
}
